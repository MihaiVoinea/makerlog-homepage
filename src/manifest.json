{
  "name": "Makerlog",
  "description": "Homepage for Makerlog, a community of independent creators that build and ship products!",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"]
  }
}
